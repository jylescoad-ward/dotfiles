{
    "files.associations": {
        "*.jsdoc": "javascript"
    },
    "workbench.iconTheme": "vscode-icons",
    "workbench.productIconTheme": "icons-carbon",
    "workbench.startupEditor": "none",
    "workbench.editorAssociations": {
        "*.wasm": "default"
    },
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "editor.bracketPairColorization.enabled": true,
    "editor.guides.bracketPairs": "active",
    "explorer.confirmDragAndDrop": false,
    "editor.semanticHighlighting.enabled": false,
    "editor.stickyTabStops": true,
    "explorer.confirmDelete": false,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "editor.fontSize": 16,
    "editor.fontFamily": "'Mensch', 'IBM Plex Mono', monospace",
    "editor.fontWeight": "normal",
    "diffEditor.codeLens": true,
    "terminal.integrated.enableBell": true,
    "typescript.format.placeOpenBraceOnNewLineForFunctions": true,
    "typescript.format.placeOpenBraceOnNewLineForControlBlocks": true,
    "typescript.format.insertSpaceBeforeFunctionParenthesis": true,
    "typescript.format.insertSpaceAfterTypeAssertion": true,
    "typescript.format.insertSpaceAfterConstructor": true,
    "typescript.format.semicolons": "remove",
    "javascript.format.placeOpenBraceOnNewLineForFunctions": true,
    "javascript.format.placeOpenBraceOnNewLineForControlBlocks": true,
    "javascript.format.insertSpaceBeforeFunctionParenthesis": true,
    "javascript.format.semicolons": "remove",
    "license.year": "2022",
    "license.default": "agpl-3.0",
    "license.author": "Kate Ward <kate@dariox.club> (https://kate.pet)",
    "license.extension": ".txt",
    "security.workspace.trust.untrustedFiles": "open",
    "vetur.format.options.tabSize": 4,
    "editor.largeFileOptimizations": false,
    "rpc.buttonInactiveUrl": "https://github.com/ktwrd",
    "rpc.appName": "Visual Studio Code",
    "typescript.format.enable": false,
    "javascript.format.enable": false,
    "javascript.inlayHints.propertyDeclarationTypes.enabled": true,
    "javascript.referencesCodeLens.enabled": true,
    "javascript.referencesCodeLens.showOnAllFunctions": true,
    "javascript.inlayHints.parameterTypes.enabled": true,
    "javascript.inlayHints.variableTypes.enabled": true,
    "javascript.preferences.importModuleSpecifierEnding": "index",
    "javascript.preferences.quoteStyle": "single",
    "javascript.inlayHints.functionLikeReturnTypes.enabled": true,
    "javascript.inlayHints.parameterNames.enabled": "all",
    "typescript.inlayHints.enumMemberValues.enabled": true,
    "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
    "spellright.addToSystemDictionary": true,
    "rpc.detailsEditing": "In {workspace}",
    "rpc.detailsViewing": "In {workspace}",
    "rpc.lowerDetailsEditing": "Working on {file_name}",
    "telemetry.telemetryLevel": "off",
    "vsicons.dontShowNewVersionMessage": true,
    "remote.SSH.remotePlatform": {
        "localhost": "linux",
        "139.99.197.10": "linux",
        "hydra": "linux"
    },
    "workbench.colorTheme": "Catppuccin Mocha",
    "workbench.preferredDarkColorTheme": "Visual Studio 2019 Dark",
    "editor.stickyScroll.enabled": true,
    "latex-workshop.linting.chktex.exec.args": [
        "-wall",
        "-n22",
        "-n30",
        "-e16",
        "-q",
        "-n46"
    ],
    "terminal.integrated.defaultProfile.linux": "bash",
    "terminal.integrated.profiles.linux": {
        "bash": {
            "path": "/usr/bin/flatpak-spawn",
            "args": ["--host", "--env=TERM=xterm-256color", "bash"]
        }
    }
}